<part>
  <display-name>News-list</display-name>
  <config>
      <input type="TextLine" name="title">
        <label>title</label>
        <occurrences minimum="1" maximum="1"/>
      </input>

      <input type="TextLine" name="alt">
        <label>subtext</label>
        <occurrences minimum="1" maximum="1"/>
      </input>

      <input type="TextLine" name="maxNews">
        <label>Number of news (100 is default)</label>
        <occurrences minimum="0" maximum="1"/>
      </input>

  </config>
</part>
