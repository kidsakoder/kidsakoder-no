<part>
  <display-name>main-button</display-name>
  <config>

      <input type="ImageSelector" name="image">
        <label>picture</label>
        <occurrences minimum="1" maximum="1"/>
      </input>


      <input type="TextLine" name="title">
        <label>title</label>
        <occurrences minimum="1" maximum="1"/>
      </input>

      <input type="TextLine" name="alt">
        <label>alt</label>
        <occurrences minimum="1" maximum="1"/>
      </input>

      <inline mixin="links"/>


  </config>
</part>
